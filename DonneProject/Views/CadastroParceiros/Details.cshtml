@model DonneProject.Models.CadastroParceiro

@{
    ViewData["Title"] = "Details";
}

<h3 class="m-4 text-center">O que deseja saber sobre a Parceria?</h3>

<div class="container-fluid">
    <h4>Parceiro</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Razão Social
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RZsocial)
        </dd>
        <dt class = "col-sm-2">
            Nome do Representante
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class = "col-sm-2">
            CNPJ
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.cnpj)
        </dd>
        <dt class = "col-sm-2">
            E-mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.email)
        </dd>
        <dt class = "col-sm-2">
            Área de Atuação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeArea.NomeArea)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-info ml-2" asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Lista Completa</a>
</div>
